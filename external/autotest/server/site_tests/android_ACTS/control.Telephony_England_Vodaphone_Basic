AUTHOR = 'bettyzhou, krisr'
NAME = 'android_ACTS.Telephony_England_Vodaphone_Basic'
TIME = 'MEDIUM'
TEST_TYPE = 'Server'
ATTRIBUTES = 'suite:android_telephony_international_roaming'
DEPENDENCIES = 'carrier:england_vodaphone'

DOC = """
This test runs telephony sanity against phones with England Vodaphone SIMs.
"""

def run(machine):
    job.run_test('android_ACTS',
                 testbed=hosts.create_testbed(machine),
                 config_file='android1758-telephony-test-station.config',
                 test_file='telephony_uk_vodafone',
                 additional_configs=['simcard_list.json'],
                 acts_timeout=18000)

parallel_simple(run, machines)
